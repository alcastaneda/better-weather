RxiBSHshGvBe4cVqaWTeRArt4iZuszgwiZCj0YEBSMe\",\n    secret: \"l0SvbkL5Whp5GSWU22fjVJutNJgyds22UpsX7dOb\",\n    version: \"2\"\n  });\n\n  async function requestPets() {\n    const animals = await petApi.pet.find({\n      location,\n      breed,\n      type: animal\n    });\n    console.log(animals.petfinder.pets.pet);\n    setPets(animals.petfinder.pets.pet || []);\n  }\n\n  useEffect(() => {\n    updateBreeds([]);\n    updateBreed(\"\");\n    petApi.breed.list({ animal }).then(results => {\n      const breedStrings = results.petfinder.breeds.breed;\n      updateBreeds(breedStrings);\n    }, console.error);\n  }, [animal]);\n\n  return (\n    <div className=\"search-params\">\n      <form\n        onSubmit={e => {\n          e.preventDefault();\n          requestPets();\n        }}\n      >\n        <label htmlFor=\"location\">\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={e => setLocation(e.target.value)}\n          />\n        </label>\n        <AnimalDropdown />\n        <BreedDropdown />\n        <button>Submit</button>\n      </form>\n      <Results pets={pets} />\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"hash":"12e9ee1441791de30a275035b04cf480","cacheData":{"env":{}}}